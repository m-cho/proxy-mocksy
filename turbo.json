{
  "$schema": "https://turborepo.com/schema.json",
  "tasks": {
    "build": {
      "dependsOn": ["^build"],
      "outputs": ["out/**"]
    },
    "start": {
      "dependsOn": ["^build"]
    },
    "docker:build": {
      "cache": false,
      "outputs": []
    },
    "docker:publish": {
      "cache": false,
      "outputs": []
    },
    "docker:startWithExample": {
      "cache": false,
      "outputs": []
    }
  }
}