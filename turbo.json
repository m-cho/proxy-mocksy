{
  "$schema": "https://turborepo.com/schema.json",
  "tasks": {
    "build": {
      "dependsOn": ["^build"],
      "outputs": ["out/**"]
    },
    "start": {
      "dependsOn": ["^build"]
    },
    "docker:build": {
      "cache": false,
      "outputs": []
    },
    "docker:publish": {
      "cache": false,
      "outputs": []
    },
    "docker:startWithExample": {
      "cache": false,
      "outputs": []
    },
    "jsr:publish": {
      "dependsOn": ["^jsr:publish"],
      "cache": false,
      "outputs": []
    },
    "jsr:publish-only": {
      "cache": false,
      "outputs": []
    },
    "jsr:dry-run": {
      "dependsOn": ["^jsr:dry-run"],
      "cache": false,
      "outputs": []
    },
    "jsr:dry-run-only": {
      "cache": false,
      "outputs": []
    }
  }
}