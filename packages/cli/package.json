{
  "name": "@proxy-mocksy/cli",
  "description": "CLI tool for running local mock API servers with JSON configuration",
  "version": "0.0.15",
  "private": false,
  "type": "module",
  "main": "./out/cli.js",
  "bin": {
    "proxy-mocksy": "out/cli.js"
  },
  "files": [
    "/out"
  ],
  "scripts": {
    "build": "tsc -p ./",
    "start": "node ./out/cli.js",
    "test": "echo \"Error: no test specified\" && exit 1",
    "clean": "tsc -b --clean",
    "deno:test": "deno run --import-map=import-map.json --unstable-sloppy-imports --allow-net --allow-read --allow-env ./src/cli.ts",
    "deno:test:config": "deno run --import-map=import-map.json --unstable-sloppy-imports --allow-net --allow-read --allow-env ./src/cli.ts --config ../../examples/config/proxy-mocksy.config.json",
    "jsr:publish": "npx jsr publish",
    "jsr:publish-only": "npx jsr publish",
    "jsr:dry-run": "npx jsr publish --dry-run",
    "jsr:dry-run-only": "npx jsr publish --dry-run"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/m-cho/proxy-mocksy.git"
  },
  "author": "Milan Stojimirov",
  "license": "LGPL-2.1-only",
  "bugs": {
    "url": "https://github.com/m-cho/proxy-mocksy/issues"
  },
  "homepage": "https://github.com/m-cho/proxy-mocksy#readme",
  "dependencies": {
    "@needle-di/core": "^1.0.0",
    "@proxy-mocksy/core": "*",
    "commander": "^14.0.0"
  }
}
